{
  "name": "api-mock",
  "version": "1.0.5",
  "description": "A tool to mock api.",
  "main": "server.js",
  "scripts": {
    "test": "jasmine test/*.js",
    "start": "node server.js",
    "dev": "supervisor server.js",
    "lint": "eslint server.js src/**.js",
    "commit-msg-check": "validate-commit-msg",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shushanfx/api-mock.git"
  },
  "keywords": [
    "mock",
    "api"
  ],
  "author": "shushanfx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shushanfx/api-mock/issues"
  },
  "homepage": "https://github.com/shushanfx/api-mock#readme",
  "dependencies": {
    "config": "^1.27.0",
    "is-html": "^1.1.0",
    "is-ip": "^2.0.0",
    "koa": "^2.6.2",
    "koa-body": "^4.0.4",
    "koa-router": "^7.2.1",
    "koa-views": "^6.1.1",
    "koa2-file-middle": "^1.0.1",
    "log4js": "^2.3.11",
    "merge": "^1.2.1",
    "mockjs": "^1.0.1-beta3",
    "mongoose": "^4.13.17",
    "parse-domain": "^2.1.7",
    "pug": "^2.0.0-rc.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "socket.io": "^2.2.0",
    "url-pattern": "^1.0.3",
    "urlencode": "^1.1.0"
  },
  "devDependencies": {
    "conventional-changelog": "^3.1.8",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^2.2.0",
    "validate-commit-msg": "^2.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "npm run commit-msg-check"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
